{
    "name": "frozzare/features",
    "type": "utility",
    "license": "MIT",
    "description": "Simple package for feature flag-driven development",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Frozzare\\Features\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Frozzare\\Tests\\Features\\": "tests/"
        }
    }
}
